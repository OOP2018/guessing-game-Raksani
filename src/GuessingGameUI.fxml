<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GuessController">
   <children>
      <TitledPane animated="false" prefHeight="400.0" prefWidth="600.0" text="Guessing Game">
         <content>
            <VBox prefHeight="372.0" prefWidth="516.0">
               <children>
                  <Label alignment="CENTER" prefHeight="42.0" prefWidth="317.0" text="Guess the Secret  Number" textAlignment="CENTER">
                     <VBox.margin>
                        <Insets left="150.0" top="50.0" />
                     </VBox.margin>
                     <font>
                        <Font name="System Bold Italic" size="24.0" />
                     </font></Label>
                  <Label fx:id="label" alignment="CENTER" prefHeight="32.0" prefWidth="236.0" textAlignment="CENTER" textOverrun="CENTER_WORD_ELLIPSIS">
                     <VBox.margin>
                        <Insets bottom="10.0" left="180.0" top="30.0" />
                     </VBox.margin></Label>
                  <TextField fx:id="textfield1" maxWidth="146.0" minHeight="20.0" minWidth="50.0" onAction="#guess" prefHeight="40.0" prefWidth="146.0" promptText="Enter secret number">
                     <VBox.margin>
                        <Insets left="225.0" top="30.0" />
                     </VBox.margin>
                  </TextField>
                  <HBox alignment="CENTER" prefHeight="24.0" prefWidth="129.0" spacing="20.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#resetSecret" text="Restart" />
                        <Button mnemonicParsing="false" onAction="#giveUp" text="Give up" />
                     </children>
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <VBox.margin>
                        <Insets left="20.0" top="30.0" />
                     </VBox.margin>
                  </HBox>
               </children></VBox>
         </content>
      </TitledPane>
   </children>
</AnchorPane>
